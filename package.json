{
  "name": "stellar-name",
  "version": "1.0.0",
  "description": "Generate unique slugs from star adjectives and named star combinations",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "slug",
    "stars",
    "names",
    "generator",
    "unique",
    "random"
  ],
  "author": {
    "name": "Vitalii Petrenko",
    "url": "https://vitalii4reva.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitalii4reva/stellar-name.git"
  },
  "homepage": "https://github.com/vitalii4reva/stellar-name#readme",
  "bugs": {
    "url": "https://github.com/vitalii4reva/stellar-name/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
